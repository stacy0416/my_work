net: "autoencoder_init.prototxt"
test_iter: 8000
test_interval: 10000
test_compute_loss: true
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 10000
display: 100
max_iter: 64000
weight_decay: 0.005
snapshot: 1000
snapshot_prefix: "snapshot/"
momentum: 0.9
# solver mode: CPU or GPU
solver_mode: GPU

solver_type: SGD
